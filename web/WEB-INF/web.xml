<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>GuessNumberGame</display-name>
    <description>Guess number game</description>
    <welcome-file-list>
        <welcome-file>form.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
        <init-param>
            <description></description>
            <param-name>helloween</param-name>
            <param-value>password</param-value>
        </init-param>
        <init-param>
            <description></description>
            <param-name>admin</param-name>
            <param-value>admin123456</param-value>
        </init-param>
        <init-param>
            <description></description>
            <param-name>babyface</param-name>
            <param-value>babyface</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
</web-app>